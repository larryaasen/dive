# When starting app from Xcode, use attach to connect VS Code to the running
# Flutter app.
attach:
	flutter attach --target=lib/main_example1.dart --debug-uri http://127.0.0.1:64830/Y82puNvoaBc=/

attach_example1:
	flutter attach --target=lib/main_example1.dart --debug-uri http://127.0.0.1:64830/Y82puNvoaBc=/

run:
	flutter run lib/main_example3.dart